{% extends 'base.html.twig' %}

{% block title %} Category list {% endblock %}

{% block body %}
    Category list:
    {% for category in categories %}
        <h4><a href="{{ path('category_show', {'categoryName': category.name}) }}">{{ category.name }}</a></h4>
    {% endfor %}
    <a href="{{ path('category_index') }}">Return to home page</a>
{% endblock %}